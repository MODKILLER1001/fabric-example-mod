{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",

  "name": "Example Mod",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "MODKILLER1001"
  ],
  "contact": {
    "homepage": "https://github.com/MODKILLER1001/fabric-example-mod/blob/master/README.md",
    "sources": "https://github.com/MODKILLER1001/fabric-example-mod",
    "issues": "https://github.com/MODKILLER1001/fabric-example-mod/issues"
  },

  "license": "WTFPL",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.fabricmc.example.ExampleMod"
    ]
  },
  "mixins": [
    "modid.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {},

  "custom": {
    "modmenu": {
      "links": {
        "modmenu.modrinth": "https://modrinth.com/mod/fabric-example-mod",
        "modmenu.curseforge": "https://www.curseforge.com/minecraft/mc-mods/fabric-example-mod"
      }
    }
  }
}
